<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="50.0" minHeight="50.0" prefHeight="50.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.GUI.controller.ControllerThongtintaikhoanPost">
   <children>
      <HBox prefHeight="50.0" prefWidth="900.0">
         <children>
            <Label alignment="CENTER" maxHeight="50.0" minHeight="50.0" prefHeight="50.0" prefWidth="42.0" text="Địa chỉ:">
               <font>
                  <Font size="13.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Label>
            <JFXTextField fx:id="thongtintaikhoan_post_txt_diachi" disable="true" maxHeight="50.0" minHeight="50.0" prefHeight="50.0" prefWidth="600.0">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font size="13.0" />
               </font>
            </JFXTextField>
            <JFXButton fx:id="thongtintaikhoan_post_btn_sua" maxHeight="30.0" minHeight="30.0" onAction="#switchToDetail" prefHeight="30.0" prefWidth="100.0" text="Sửa">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="13.0" />
               </font>
            </JFXButton>
            <JFXButton fx:id="thongtintaikhoan_post_btn_xoa" maxHeight="30.0" onAction="#deleteDetail" minHeight="30.0" prefHeight="30.0" prefWidth="100.0" text="Xóa">
               <HBox.margin>
                  <Insets left="10.0" top="10.0" />
               </HBox.margin>
               <font>
                  <Font size="13.0" />
               </font>
            </JFXButton>
         </children>
      </HBox>
   </children>
</AnchorPane>
